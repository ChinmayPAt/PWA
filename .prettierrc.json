{
  "useTabs": false,
  "htmlWhitespaceSensitivity": "ignore",
  "singleAttributePerLine": false,
  "singleQuote": true,
  "semi": true,
  "bracketSpacing": true,
  "arrowParens": "avoid",
  "trailingComma": "es5",
  "bracketSameLine": true,
  "printWidth": 100,
  "overrides": [
    {
      "files": "*.html",
      "options": {
        "parser": "angular",
        "useTabs": false
      }
    }
  ]
}
